{
    "name": "highwire/scolaris-site-hwjma",
    "type": "project",
    "description": "Scolaris hwjma site instance",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "process-timeout": 0,
        "theme-directories": [
            "web/modules/freebird/themes/highwire"
        ],
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "composer/installers": true,
            "zaporylie/composer-drupal-optimizations": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "highwire/freebird": "dev-10.x-1.1-dev",
        "highwire/freebird-scaffold": "dev-10.x-1.x-dev",
        "drupal/view_unpublished": "1.1.0",
        "drupal/profile_switcher": "1.0.0-alpha6",
        "whichbrowser/parser": "v2.1.7",
        "composer/installers": "^2.0",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "cweagans/composer-patches": "1.7.3",
        "zaporylie/composer-drupal-optimizations": "1.2.0",
        "vlucas/phpdotenv": "v4.3.0",
        "bower-asset/jquery-ui-slider-pips": "v1.11.5",
        "laminas/laminas-code": "4.11.0",
        "roave/better-reflection": "6.13.0",
        "dmore/chrome-mink-driver": "2.9.2",
        "phayes/elasticentityquery": "dev-9.x-4.x-dev-rachit",
        "drupal/advagg": "6.0.0-alpha1",
        "twig/twig": "3.6.0"
    },
    "require-dev": {
        "behat/behat": "v3.13.0",
        "behat/mink": "v1.10.0",
        "behat/mink-selenium2-driver": "v1.6.0",
        "behat/mink-extension": "2.3.1",
        "behat/mink-goutte-driver": "v1.4.0",
        "mikey179/vfsstream": "v1.6.11",
        "behat/mink-zombie-driver": "v1.6.0",
        "symfony/psr-http-message-bridge": "2.2.0"
    },
    "extra": {
        "patches": {
            "drupal/search_api_solr": {
                "Add term autocomplete alter query hook support": "https://smart.highwire.org/sites/default/files/search-api-autocomplete-term-suggestions.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "freebird": {
            "type": "git",
            "url": "git@github.com:highwire/freebird.git"
        },
        "freebird-scaffold": {
            "type": "git",
            "url": "git@github.com:highwire/freebird-scaffold.git"
        },
        "hwphp": {
            "type": "git",
            "url": "git@github.com:highwire/hwphp.git"
        },
        "elasticeq": {
            "type": "git",
            "url": "git@github.com:highwire/elasticentityquery.git"
        },
        "marcrecords": {
            "type": "git",
            "url": "git@github.com:highwire/marc-records-client-php.git"
        },
        "pdata": {
            "type": "git",
            "url": "git@github.com:highwire/personalization-data-client-php.git"
        },
        "pclient": {
            "type": "git",
            "url": "git@github.com:highwire/personalization-client-php.git"
        },
        "casatoken": {
            "type": "git",
            "url": "git@github.com:highwire/casa-token-client-php.git"
        },
        "transfertoken": {
            "type": "git",
            "url": "https://github.com/highwire/transfer-token-client-php.git"
        },
        "ckeditor": {
             "type": "package",
             "package": {
                 "name": "ckeditor-plugin/fakeobjects",
                 "version": "4.14.0",
                 "type": "drupal-library",
                 "dist": {
                    "url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.14.0.zip",
                    "type": "zip"
                  },
                  "require": {
                      "composer/installers": "^2.0"
                  }
               }
         },
        "ckeditor_link": {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/link",
                "version": "4.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/link/releases/link_4.14.0.zip",
                    "type": "zip"
                 },
                 "require": {
                     "composer/installers": "^2.0"
                 }
            }
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/FreebirdHandler.php"
        ]
    },
    "scripts": {
        "theme:compile": "FreebirdComposer\\CommandHandler::compileThemes",
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "FreebirdComposer\\CommandHandler::provisionSettings",
            "HighWireSite\\composer\\FreebirdHandler::setupFreebirdCode",
            "HighWireSite\\composer\\FreebirdHandler::overridePatternLabConfig",
            "@theme:compile"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "FreebirdComposer\\CommandHandler::provisionSettings",
            "HighWireSite\\composer\\FreebirdHandler::setupFreebirdCode",
            "HighWireSite\\composer\\FreebirdHandler::overridePatternLabConfig",
            "@theme:compile"
        ]
    }
}
