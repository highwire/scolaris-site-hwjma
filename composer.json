{
    "name": "highwire/hwjma",
    "description": "Site specific repository for hwjma",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "process-timeout": 0,
        "theme-directories": [
            "web/modules/freebird/themes/highwire"
        ]
    },
    "require": {
        "highwire/freebird": "10.x-1.x-dev",
        "highwire/freebird-scaffold": "dev-9.x-4.x-dev-rachit",
        "drupal/view_unpublished": "1.1.0",
        "drupal/profile_switcher": "1.0.0-alpha6",
        "whichbrowser/parser": "v2.1.7",
        "drupal/devel": "4.2.1",
        "drush/drush": "10.6.2",
        "drupal/drupal-driver": "v2.2.2",
        "drupal/core-composer-scaffold": "9.5.10",
        "cweagans/composer-patches": "1.7.3",
        "zaporylie/composer-drupal-optimizations": "1.2.0",
        "vlucas/phpdotenv": "v4.3.0",
        "bower-asset/jquery-ui-slider-pips": "v1.11.5",
        "laminas/laminas-code": "4.11.0",
        "roave/better-reflection": "6.13.0",
        "cache/array-adapter": "1.2.0",
        "dmore/chrome-mink-driver": "2.9.2",
        "phayes/elasticentityquery": "dev-9.x-4.x-dev-rachit",
        "drupal/advagg": "5.0.0",
        "drupal/eme": "1.0.0-alpha13" 
   },
    "require-dev": {
        "sensiolabs/behat-page-object-extension": "v2.3.7",
        "behat/behat": "v3.13.0",
        "behat/mink": "v1.10.0",
        "behat/mink-selenium2-driver": "v1.6.0",
        "behat/mink-extension": "2.3.1",
        "behat/mink-goutte-driver": "v1.3.0",
        "drupal/drupal-extension": "v4.2.1",
        "jcalderonzumba/gastonjs": "v1.2.0",
        "jcalderonzumba/mink-phantomjs-driver": "v0.3.3",
        "mikey179/vfsstream": "v1.6.11",
        "symfony/css-selector": "v4.4.44",
        "behat/mink-zombie-driver": "v1.6.0"
    },
    "extra": {
        "patches": {
            "drupal/search_api_solr": {
                "Unable to uninstall base theme and subtheme at the same time.": "https://www.drupal.org/files/issues/2020-10-28/3001430-5.patch"
            }
        },        
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "freebird": {
            "type": "git",
            "url": "git@github.com:highwire/freebird.git"
        },
        "freebird-scaffold": {
            "type": "git",
            "url": "git@github.com:highwire/freebird-scaffold.git"
        },
        "hwphp": {
            "type": "git",
            "url": "git@github.com:highwire/hwphp.git"
        },
        "elasticeq": {
            "type": "git",
            "url": "git@github.com:phayes/elasticentityquery.git"
        },
        "marcrecords": {
            "type": "git",
            "url": "git@github.com:highwire/marc-records-client-php.git"
        },
        "pdata": {
            "type": "git",
            "url": "git@github.com:highwire/personalization-data-client-php.git"
        },
        "pclient": {
            "type": "git",
            "url": "git@github.com:highwire/personalization-client-php.git"
        },
        "casatoken": {
            "type": "git",
            "url": "git@github.com:highwire/casa-token-client-php.git"
        },
        "transfertoken": {
            "type": "git",
            "url": "https://github.com/highwire/transfer-token-client-php.git"
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/FreebirdHandler.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "FreebirdComposer\\CommandHandler::provisionSettings",
            "HighWireSite\\composer\\FreebirdHandler::setupFreebirdCode"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
            "FreebirdComposer\\CommandHandler::provisionSettings",
            "HighWireSite\\composer\\FreebirdHandler::setupFreebirdCode"
        ]
    }
}
